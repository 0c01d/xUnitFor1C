	ПостроительДереваТестов = КонтекстЯдра.Плагин(""ПостроительДереваТестов"");
	Элемент1 = КонтекстЯдра.Плагин(""ПостроительДереваТестов"").СоздатьЭлемент(ПолучитьИспользуемоеИмяФайла(), ""УспешныйМетод"");
